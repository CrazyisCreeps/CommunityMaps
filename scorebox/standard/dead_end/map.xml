<map proto="1.4.2">
<name>Dead End</name>
<version>1.0.3</version>
<objective>Jump into the enemy team's portal, located below their spawn point!</objective>
<created>2023-05-07</created>
<include id="bridge"/>
<constants>
    <constant id="score-limit">5</constant>
    <constant id="score-mode">static</constant> <!-- static, mercy -->
    <constant id="spawn-cage-mode">default</constant> <!-- default (3x3 stained glass cube), custom -->
    <!-- Teams -->
    <constant id="team-one-spawn">-39.5,14,0.5</constant>
    <constant id="team-one-yaw">-90</constant>
    <constant id="team-one-pitch">0</constant>
    <constant id="team-two-spawn">40.5,14,0.5</constant>
    <constant id="team-two-yaw">90</constant>
    <constant id="team-two-pitch">0</constant>
</constants>
<authors>
    <author uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128"/> <!-- zzuf -->
</authors>
<contributors>
    <contributor uuid="d5a64b26-15ea-46c4-8319-f9c83e82a1fa"/> <!-- Eliminate -->
</contributors>
<terrain environment="nether"/>
<teams>
    <team id="team-one" color="yellow" max="5">Yellow</team>
    <team id="team-two" color="dark purple" max="5">Purple</team>
</teams>
<spawns>
    <default>
        <region>
            <point yaw="0">0.5,23,-33.5</point>
        </region>
    </default>
</spawns>
<filters>
    <not id="other-blocks">
        <any id="some-blocks">
            <material>stained clay:10</material>
            <material>stained clay:4</material>
            <material>stained clay:0</material>
        </any>
    </not>
</filters>
<regions>
    <negative id="not-buildable">
        <cuboid min="-27,1,15" max="28,14,-14"/>
    </negative>
    <apply block="some-blocks"/>
    <!-- Portals -->
    <cuboid id="team-one-portal" min="-37,4,-1" size="3,1,3"/>
    <mirror id="team-two-portal" region="team-one-portal" origin="0.5,0,0.5" normal="1,0,0"/>
    <!-- Spawn points -->
    <union id="cages">
        <!-- Edit if using custom cages, otherwise delete -->
    </union>
    <!-- Dead End -->
    <apply velocity="1,1,0">
        <region>
            <cuboid id="team-one-jump-pad" min="-50,9,-1" size="2,1,3"/>
        </region>
    </apply>
    <apply velocity="-1,1,0">
        <region>
            <mirror region="team-one-jump-pad" origin="0.5,0,0.5" normal="1,0,0"/>
        </region>
    </apply>
    <apply use="never">
        <region>
            <block>0,24,-37</block>
        </region>
    </apply>
</regions>
</map>
